{
  "expo": {
    "name": "Docuvault",
    "slug": "Docuvault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "1.0.0",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.docuvault",
      "infoPlist": {
        "NSFaceIDUsageDescription": "We use Face ID to unlock your secure vault.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.docuvault"
    },
    "extra": {
      "eas": {
        "projectId": "cdc78a03-a11b-408c-a1ae-7168f274d490"
      }
    },
    "plugins": [
      "expo-font",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "DocuVault needs access to your camera to scan documents.",
          "enableMicrophonePermission": false
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true
        }
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/4410e80c-3a1e-41fd-9f43-bbc7f250c51e"
    },
    "owner": "razer_ak"
  }
}